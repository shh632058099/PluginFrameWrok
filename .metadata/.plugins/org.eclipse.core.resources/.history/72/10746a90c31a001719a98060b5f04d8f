#include "module.h"
#include "ControllerBase.h"
#include "ViewBase.h"

Module::Module(const std::string &strName)
{
}

Module::~Module(void)
{
	if (m_pView != NULL)
	{
		delete m_pView;
		m_pView = NULL;
	}

	if (m_pController != NULL)
	{
		delete m_pController;
		m_pController = NULL;
	}
}

